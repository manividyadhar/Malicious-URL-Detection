{
    "name": "malicious-url-detector",
    "version": "1.0.0",
    "description": "Comprehensive malicious URL detection system",
    "private": true,
    "workspaces": [
        "shared",
        "server",
        "client",
        "cli"
    ],
    "scripts": {
        "build": "npm run build:shared && npm run build:server && npm run build:client",
        "build:shared": "cd shared && npm install && npm run build",
        "build:server": "cd server && npm install && npm run build",
        "build:client": "cd client && npm install && npm run build",
        "start": "cd server && npm start",
        "dev:server": "cd server && npm run dev",
        "dev:client": "cd client && npm run dev",
        "test": "cd shared && npm test"
    },
    "keywords": [
        "url-detection",
        "phishing",
        "security",
        "malware-detection"
    ],
    "author": "",
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}